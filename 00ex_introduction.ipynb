   "source": [
    "import math\n",
    "\n",
    "u = [3, 0]\n",
    "v = [0, 4]\n",
    "\n",
    "def compute_distance(p1, p2):\n",
    "    # Tried both ways for the exponential (same result)\n",
    "    d = math.sqrt(pow((p1[0]-p2[0]),2) + ((p1[1]-p2[1])**2))\n",
    "    return d\n",
    "\n",
    "print(compute_distance(u, v))"
