    "def normalization():\n",
    "    # empty list \n",
    "    lst = [] \n",
    "    # number of elements as input \n",
    "    n = int(input(\"Enter number of elements : \")) \n",
    "    #insert elements\n",
    "    for i in range(0, n): \n",
    "        element = float(input()) \n",
    "        lst.append(element) # adding the element \n",
    "        \n",
    "    s = sum(lst)\n",
    "    lst = [i/s for i in lst]\n",
    "\n",
    "    return(tuple(lst))\n",
    "\n",
    "tpl = normalization()\n",
    "\n",
