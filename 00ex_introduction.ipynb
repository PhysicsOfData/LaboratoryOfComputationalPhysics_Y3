    "a = (1, 0.3, 3.7, 4.2, 0.4, 2.2)\n",
    "b = (2,3,4,5,6,7)\n",
    "def tup_normal(var):\n",
    "    \n",
    "    list_a = list(var)    \n",
    "    var_max = max(var)\n",
    "    var_min = min(var)\n",
    "    \n",
    "    for i,num in enumerate(list_a):\n",
    "        list_a[i] = (num-var_min)/(var_max - var_min)\n",
    "    \n",
    "    tup_a = tuple(list_a)\n",
    "    return tup_a\n",
    "\n",
    "print(tup_normal(a))\n",
