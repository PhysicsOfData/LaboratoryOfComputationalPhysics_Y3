    "def counter(ss):\n",
    "    s=ss.casefold()\n",
    "    uniques=dict()\n",
    "    for char in s:\n",
    "        if char not in uniques.keys():\n",
    "            uniques[char]=1\n",
    "        else:\n",
    "            uniques[char]+=1            \n",
    "    return uniques\n",
    "\n",
    "print(counter(s))"
