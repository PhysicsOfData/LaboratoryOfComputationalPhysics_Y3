    "4\\. Write the factorial function so that it a) does and b) does not use recursion."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Case a\n",
      "The factorial of  5 is 120\n",
      "\n",
      "\n",
      "Case b\n",
      "The factorial of  6 is 720\n"
     ]
    }
   ],
   "source": [
    "#code ex4\n",
    "\n",
    "#a does use recursion\n",
    "print(\"Case a\")\n",
    "\n",
    "a=5 #insert number\n",
    "def recfact(x): #must exclude 0 or in the recursion x-1 will be = 0\n",
    "    if x == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return x*recfact(x-1)\n",
    "\n",
    "print(\"The factorial of \",a, \"is\", recfact(a))\n",
    "print(\"\\n\")\n",
    "\n",
    "\n",
    "#b does not use recursion\n",
    "print(\"Case b\")\n",
    "\n",
    "b= 6  #insert number \n",
    "def factorial(n):\n",
    "    i = n\n",
    "    while i>1:\n",
    "        i=i-1\n",
    "        n=n*i\n",
    "    return n\n",
    "\n",
    "print(\"The factorial of \",b, \"is\", factorial(b))\n",
    "        \n",
    "\n",
    "        "
   ]
