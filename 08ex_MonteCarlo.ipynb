    "def w(x):\n",
    "    return x**(-1/2)\n",
    "    \n",
    "def integralW(x):\n",
    "    return 2*x**(1/2)\n",
    "\n",
    "def f(x):\n",
    "    return w(x)/(np.e**x+1)\n",
    "\n",
    "def g(x):\n",
    "    return 1/(np.e**x+1)\n",
    "\n",
    "plt.plot(np.arange(0,30),w(np.arange(0,30)), '.')\n",
    "plt.plot(np.arange(0,30),f(np.arange(0,30)), '.')\n",
    "plt.plot(np.arange(0,30),g(np.arange(0,30)), '.')\n",
    "plt.legend(['w','f','g'])\n",
    "\n",
