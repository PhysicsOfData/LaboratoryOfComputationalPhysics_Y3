    "def find_tot (L):\n",
    "    size = len(L)\n",
    "    tot = 0\n",
    "    for i in range(0,size,1):\n",
    "        tot = tot + L[i]\n",
    "    \n",
    "    return tot\n",
    "\n",
    "def norm (L):\n",
    "    nL = []\n",
    "    max = find_tot(L)\n",
    "    size = len(L)\n",
    "    for i in range(0,size,1):\n",
    "        nL.append(L[i]/max)\n",
    "        \n",
    "    return nL\n",
    "\n",
    "\n",
    "test = [0.1, 1.2, 0.675, 0.22, 0.32]\n",
    "\n",
    "result = norm(test)\n",
    "\n",
    "print(test)\n",
    "print(result)\n",
